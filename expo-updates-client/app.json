{
  "expo": {
    "name": "ධර්මය",
    "slug": "dharma",
    "owner": "anonymous",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "dharma",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": "2",
    "updates": {
      "url": "http://192.168.1.13:3000/api/manifest",
      "enabled": true,
      "fallbackToCacheTimeout": 30000,
      "codeSigningCertificate": "./code-signing/certificate.pem",
      "codeSigningMetadata": {
        "keyid": "main",
        "alg": "rsa-v1_5-sha256"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": ["ios", "android"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.sdmx.dharma"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#F5F5F5"
      },
      "edgeToEdgeEnabled": true,
      "package": "dev.sdmx.dharma"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "backgroundColor": "#F5F5F5",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#111827"
          },
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          },
          "ios": {}
        }
      ]
    ]
  }
}
